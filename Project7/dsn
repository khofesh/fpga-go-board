    %store/vec4 v0x5609a7aedee0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5609a7aedee0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5609a7aede00_0;
    %load/vec4 v0x5609a7aedee0_0;
    %part/s 1;
    %assign/vec4 v0x5609a7aee080_0, 0;
    %delay 860000, 0;
